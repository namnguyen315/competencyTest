{
  "extends": ["react-app", "prettier"],
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "singleQuote": true,
        "trailingComma": "all",
        "semi": false,
        "tabWidth": 2,
        "printWidth": 80,
        "jsxSingleQuote": false,
      },
    ],
  },
}
